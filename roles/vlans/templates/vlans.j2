{% set device_vars = interfaces[ansible_hostname] %}
{% set test_vars = vlan2[ansible_hostname] %}
auto bridge
iface bridge
        bridge-ports {{device_vars.if}}
        bridge-vids 
        bridge-vlan-aware yes {{vlans}}
        test {{ device_vars.vlans }}
        test2 {{test_vars.test1 }}
        test3 {{device_vars.vlan}}{{test_vars.test1}}
